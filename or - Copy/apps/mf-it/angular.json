{
	"version": 1,
	"defaultProject": "it-app",
	"projects": {
		"it-app": {
			"projectType": "application",
			"root": "",
			"sourceRoot": "src",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/it-app",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": [],
						"tsConfig": "tsconfig.app.json"
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "it-app:build"
					}
				}
			}
		}
	}
} 